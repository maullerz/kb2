{
  "name": "@maullerz/kb2",
  "version": "1.0.0",
  "description": "EveTools Killboard",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "node config/webpackDevServer.js",
    "build": "webpack --config ./config/webpack.prod.config.js",
    "build:stats": "webpack --config config/webpack.prod.config.js --profile --json > stats.json",
    "analyze": "npm run build:stats && npm run build && mv ./stats.json ./dist && npx webpack-bundle-analyzer ./dist/stats.json",
    "docker": "docker-compose up --build web",
    "lint": "eslint -c config/.eslintrc.js --ext js src",
    "lint:fix": "eslint -c config/.eslintrc.js --fix --ext js src",
    "lint:staged": "eslint -c config/.eslintrc.js --fix --ext js",
    "lint:commit": "`npm bin`/lint-staged",
    "lint:css": "stylelint --config config/stylelint.css.config.js --fix 'src/**/*.pcss'",
    "lint:styled": "stylelint --config config/stylelint.styled.config.js 'src/**/styles.js'"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.14.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:staged",
      "git add"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
