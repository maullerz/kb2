{
  "name": "@maullerz/kb2",
  "version": "1.0.0",
  "description": "EveTools Killboard",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "node config/webpackDevServer.js",
    "build": "`npm bin`/webpack --config ./config/webpack.prod.config.js",
    "build:stats:old": "webpack --config config/webpack.prod.config.js --profile --json > stats.json",
    "build:stats": "webpack --config config/webpack.prod.config.js --stats=detailed --json > ./stats.json",
    "analyze": "npm run build:stats && npm run build && mv ./stats.json ./dist && npx webpack-bundle-analyzer ./dist/stats.json",
    "docker": "docker-compose up --build kb2",
    "lint": "eslint -c config/.eslintrc.js --ext js src",
    "lint:fix": "eslint -c config/.eslintrc.js --fix --ext js src"
  },
